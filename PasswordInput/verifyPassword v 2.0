boolean verifyPasswords()
    {
        char password1[]=jPasswordField1.getPassword();
        char password2[]=jPasswordField2.getPassword();
        if(password1.length<8)
        {
            pwErrorLabel.setText("Password must be at least 8 characters");
            pwErrorLabel.setVisible(true);
            return false;
        }
        if(password1.length>7)
        {
            pwErrorLabel.setVisible(false);
            if(password1.length==password2.length)
            {
                pwErrorLabel.setVisible(false);
                if(Arrays.equals(password1, password2))
                {
                    return true;
                }

            }
            pwErrorLabel.setText("Passwords do not match");
            pwErrorLabel.setVisible(true);
        }
   
        return false;
		 //checking if password contain at leas one digit
		if( password.matches("(?=.*[0-9]).*") )
        {
            pwErrorLabel.setVisible(false);
            if(password1.match==password2.match)
            {
                pwErrorLabel.setVisible(false);
                if(Arrays.equals(password1, password2))
                {
                    return true;
                }

            }
            pwErrorLabel.setText("Password must contain at least one digit");
            pwErrorLabel.setVisible(true);
        }
        return false;
		//checking if password contain at leas one spacial character
		if( password.matches("(?=.*[~!@#$%^&*()_-]).*") )
        {
            pwErrorLabel.setVisible(false);
            if(password1.match==password2.match)
            {
                pwErrorLabel.setVisible(false);
                if(Arrays.equals(password1, password2))
                {
                    return true;
                }

            }
            pwErrorLabel.setText("Password must contain at least one special character");
            pwErrorLabel.setVisible(true);
        }
        return false;
		//checking if password contain at leas one upper case
		if( password.matches("(?=.*[A-Z]).*") )
        {
            pwErrorLabel.setVisible(false);
            if(password1.match==password2.match)
            {
                pwErrorLabel.setVisible(false);
                if(Arrays.equals(password1, password2))
                {
                    return true;
                }

            }
            pwErrorLabel.setText("Password must contain at least one one upper case");
            pwErrorLabel.setVisible(true);
        }
        return false;
		//checking if password contain at leas one lower case
		if( password.matches("(?=.*[a-z]).*") )
        {
            pwErrorLabel.setVisible(false);
            if(password1.match==password2.match)
            {
                pwErrorLabel.setVisible(false);
                if(Arrays.equals(password1, password2))
                {
                    return true;
                }
				
            }
            pwErrorLabel.setText("Password must contain at least one one lower case");
            pwErrorLabel.setVisible(true);
        }
        return false;
        
    }
